{
  "expo": {
    "name": "collabovid",
    "slug": "collabovid",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/images/shake.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "*******************"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.aaabdulmoiz.collabovid",
      "config": {
        "googleMaps": {
          "apiKey":"********************" 
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
